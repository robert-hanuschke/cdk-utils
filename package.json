    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "commit-and-tag-version": "^12",
    "constructs": "^10.0.0",
    "eslint": "^9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "jest-junit": "^16",
    "jsii": "~5.8.0",
    "jsii-diff": "^1.115.0",
    "jsii-docgen": "^10.5.0",
